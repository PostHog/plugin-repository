      "currency_property": {
        "name": "Property key for the currency",
        "type": "string",
        "default": "currency"
      },
      "normalized_amount_property": {
        "name": "Property key for the normalized amount",
        "type": "string",
        "default": "normalized_amount"
      },
      "normalized_currency_property": {
        "name": "Property key for the normalized currency",
        "type": "string",
